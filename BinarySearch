    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int search;
    cin>>search;
    int ans=-1;
    sort(arr,arr+n);
    int lo=0,hi=n-1,mid;
    while(hi>=lo)
    {
        mid=lo+(hi-lo)/2;
        if(arr[mid]>=search)
        {
            ans=mid;
            hi=mid-1;
        }
        else if(arr[mid]<search)
        {
            lo=mid+1;
        }
    }
