#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int temp=0;
    std::vector<int> freq(n+1,0);
    int i=0;
    while(i!=n)
    {
        freq[arr[i]]++;
        i++;
    }
    for(int i=0;i<=n;i++)
    {
        if(freq[i]!=0)
        {
            cout<<i<<" occurs "<<freq[i]<<" times"<<'\n';
        }
    }
}
