#include<bits/stdc++.h>
#define sped ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL); 
#define ln '\n'
#define ll long long
#define f first
#define s second
#define loop(n) for(unsigned int i=0; i<n; ++i)
using namespace std;
const long long M=1e9+7;

//<---------------------------code begins--------------------------->

int32_t main(){
    sped
    ll n;
    cin>>n;
    int ct=0,sum=0;
    for(int i=1;i*i<=n;++i){
    	if(n%i==0){
    		cout<<i<<" "<<n/i<<ln;
    		ct+=1;
    		sum+=i;
    		if(n/i!=i){
    			sum+=n/i;
    			ct+=1;
    		}
    	}
    }
    cout<<"count: "<<ct<<" Sum: "<< sum<<ln;
}
